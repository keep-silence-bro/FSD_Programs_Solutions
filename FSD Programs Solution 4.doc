import React, \{ useState \} from "react";\par
\par
export default function Counter() \{\par
  const [count, setCount] = useState(0);\par
\par
  return (\par
    <div>\par
      <h2>Count: \{count\}</h2>\par
      <button onClick=\{() => setCount((prev) => prev + 1)\}>Increment</button>\par
      <button onClick=\{() => setCount((prev) => prev - 1)\}>Decrement</button>\par
    </div>\par
  );\par
\}\par
