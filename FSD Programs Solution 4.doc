\fs24\b 23. Student Marks Management System using Arrays\b0\par
\fs20\b Solution Code:\b0\par
\f1\fs18
<!doctype html>\par
<html lang="en">\par
<head><meta charset="utf-8" /><title>Marks Manager</title></head>\par
<body>\par
  <h1>Marks Manager</h1>\par
  <input id="mark" type="number" placeholder="Enter mark" />\par
  <button onclick="addMark()">Add</button>\par
  <p id="summary"></p>\par
  <ul id="marksList"></ul>\par
\par
  <script>\par
    const marks = [];\par
\par
    function addMark() \{\par
      const value = Number(document.getElementById("mark").value);\par
      if (!Number.isNaN(value)) marks.push(value);\par
      document.getElementById("mark").value = "";\par
      render();\par
    \}\par
\par
    function render() \{\par
      const total = marks.reduce((a, b) => a + b, 0);\par
      const avg = marks.length ? (total / marks.length).toFixed(2) : 0;\par
      const status = avg >= 40 ? "Pass" : "Fail";\par
      document.getElementById("summary").textContent = `Total: $\{total\}, Avg: $\{avg\}, Status: $\{status\}`;\par
      document.getElementById("marksList").innerHTML = marks.map(m => `<li>$\{m\}</li>`).join("");\par
    \}\par
  </script>\par
</body>\par
</html>\par
