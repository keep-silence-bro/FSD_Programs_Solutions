  <h1 id="status">Event Demo</h1>\par
  <button id="btn">Click me</button>\par
  <div id="hoverBox" style="margin-top:20px;padding:20px;border:1px solid #333;">Hover here</div>\par
  <input id="keyInput" type="text" placeholder="Type something" />\par
