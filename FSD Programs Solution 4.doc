  <script>\par
    document.getElementById("btn").onclick = () => \{\par
      document.getElementById("status").textContent = "Button clicked";\par
    \};\par
    document.getElementById("hoverBox").onmouseover = (e) => \{\par
      e.target.style.background = "#dfe6e9";\par
    \};\par
    document.getElementById("hoverBox").onmouseout = (e) => \{\par
      e.target.style.background = "transparent";\par
    \};\par
    document.getElementById("keyInput").onkeyup = (e) => \{\par
      document.getElementById("status").textContent = `Typed: $\{e.target.value\}`;\par
    \};\par
  </script>\par
</body>\par
</html>\par
