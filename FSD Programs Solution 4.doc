\fs24\b 18. Palindrome Check using JavaScript\b0\par
\fs20\b Solution Code:\b0\par
\f1\fs18
<!doctype html>\par
<html lang="en">\par
<head><meta charset="utf-8" /><title>Palindrome</title></head>\par
<body>\par
  <h1>Palindrome Checker</h1>\par
  <input id="text" type="text" placeholder="Enter text" />\par
  <button onclick="checkPalindrome()">Check</button>\par
  <p id="result"></p>\par
\par
  <script>\par
    function checkPalindrome() \{\par
      const text = document.getElementById("text").value.toLowerCase().replace(/[^a-z0-9]/g, "");\par
      const reversed = text.split("").reverse().join("");\par
      const message = text && text === reversed ? "Palindrome" : "Not a palindrome";\par
      document.getElementById("result").textContent = message;\par
    \}\par
  </script>\par
</body>\par
</html>\par
