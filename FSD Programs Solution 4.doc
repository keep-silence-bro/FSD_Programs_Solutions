\fs24\b 21. Conditional Statements Program\b0\par
\fs20\b Solution Code:\b0\par
\f1\fs18
<!doctype html>\par
<html lang="en">\par
<head><meta charset="utf-8" /><title>Grade Calculator</title></head>\par
<body>\par
  <h1>Grade Calculator</h1>\par
  <input id="marks" type="number" min="0" max="100" placeholder="Enter marks" />\par
  <button onclick="calculate()">Calculate</button>\par
  <p id="grade"></p>\par
\par
  <script>\par
    function calculate() \{\par
      const marks = Number(document.getElementById("marks").value);\par
      let grade = "";\par
\par
      if (marks >= 90) grade = "A";\par
      else if (marks >= 80) grade = "B";\par
      else if (marks >= 70) grade = "C";\par
      else if (marks >= 60) grade = "D";\par
      else grade = "F";\par
\par
      let remark = "";\par
      switch (grade) \{\par
        case "A": remark = "Excellent"; break;\par
        case "B": remark = "Very Good"; break;\par
        case "C": remark = "Good"; break;\par
        case "D": remark = "Pass"; break;\par
        default: remark = "Fail";\par
      \}\par
\par
      document.getElementById("grade").textContent = `Grade: $\{grade\} ($\{remark\})`;\par
    \}\par
  </script>\par
</body>\par
</html>\par
