\fs24\b 22. Array Demonstration Program\b0\par
\fs20\b Solution Code:\b0\par
\f1\fs18
<!doctype html>\par
<html lang="en">\par
<head><meta charset="utf-8" /><title>Array Demo</title></head>\par
<body>\par
  <h1>Array Demonstration</h1>\par
  <input id="item" type="text" placeholder="Add item" />\par
  <button onclick="addItem()">Add</button>\par
  <button onclick="removeItem()">Remove Last</button>\par
  <ul id="list"></ul>\par
\par
  <script>\par
    const arr = ["HTML", "CSS", "JS"];\par
    const list = document.getElementById("list");\par
\par
    function render() \{\par
      list.innerHTML = arr.map(item => `<li>$\{item\}</li>`).join("");\par
    \}\par
\par
    function addItem() \{\par
      const value = document.getElementById("item").value.trim();\par
      if (value) arr.push(value);\par
      document.getElementById("item").value = "";\par
      render();\par
    \}\par
\par
    function removeItem() \{\par
      arr.pop();\par
      render();\par
    \}\par
\par
    render();\par
  </script>\par
</body>\par
</html>\par
